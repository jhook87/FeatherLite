{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "functions": {
    "**": {
      "runtime": "nodejs20.x"
    }
  },
  "buildCommand": "npm run prisma:deploy && npm run build",
  "installCommand": "npm install",
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "SKIP_ENV_VALIDATION": "false"
    }
  },
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "outputDirectory": ".next"
}
